{
  "providers": [
    "awscloudformation"
  ],
  "projectName": "MarvelTrivia",
  "version": "3.1",
  "frontend": "javascript",
  "javascript": {
    "framework": "react",
    "config": {
      "SourceDir": "src",
      "DistributionDir": "build",
      "BuildCommand": "yarn tsc -p src/server/tsconfig.json",
      "StartCommand": "yarn node -r esm ./src/server/dist/server/server.js"
    }
  }
}